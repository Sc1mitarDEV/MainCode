    loadstring(game:HttpGet("https://pastebin.com/raw/wH0ZLBTw", true))()
end)

Tab3Section:NewButton("No Jumpscare", "Remove Any Monster Jumpscare", function()
            game:GetService("ReplicatedStorage").Bricks.Jumpscare:Destroy()
print("done")
  	end)

Tab3Section:NewButton("No Screech", "Avoid Screech", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/8fHpugrg", true))()
print("done")
end)

Tab3Section:NewToggle("Key Cham", "Esp The Key", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/zG27uC8k", true))()
    else
        loadstring(game:HttpGet("https://pastebin.com/raw/zG27uC8k", true))()
    end
end)

Tab3Section:NewButton("Notify when mob spawns", "ButtonInfo", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/xmDSdqpq", true))()
end)

